    <section id="wiki">
      <div class="container">
        <div class="row">
          <div class="col-lg-8 mx-auto">
            <h2>WIKI</h2>
              <p class="second"><br><br><%= wiki%><br><br>
               <a class="btn btn-outline-dark" href="http://www.filmlabs.org/wiki//fr/start" role="button">Go wiki</a>
            </p>
             </div> 
                </p>
                 </div>
            </div>
          </div>
        </section>
     <section id="register">
            <div class="container register-form">
                <div class="form col-lg-8 mx-auto">
                    <div class="note">
                        <p>Register a new member</p>
                    </div>
                      <div class="form-content">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input type="text" class="form-control" name="name" placeholder="Your Name *" value=""/>
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control" name="email"  placeholder="Email *" value=""/>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input type="text" class="form-control" name="password" placeholder="Your Password *" value=""/>
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control"name="passwordConfirmation" placeholder="Confirm Password *" value=""/>
                                </div>
                            </div>
                        </div>
                        <button type="button" class="Submit">Submit</button>
                    </div>
                </div>
            </div>
    </section>
